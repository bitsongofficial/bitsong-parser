{
  "name": "bitsong-parser",
  "description": "Parser for BitSong Blockchain",
  "version": "0.0.1",
  "author": "Angelo Recca <hello@bitsong.io>",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run watch-ts && npm run tslint",
    "build": "tsc",
    "watch-ts": "tsc-watch --onSuccess \"nodemon dist/Server.js\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/Server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bech32": "^1.1.3",
    "bignumber.js": "^8.1.1",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.3",
    "concurrently": "^4.1.1",
    "config": "^3.2.2",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "errorhandler": "^1.5.1",
    "es6-promise": "^4.2.8",
    "express": "^4.17.3",
    "express-flash": "0.0.2",
    "express-session": "^1.16.2",
    "express-validator": "^5.3.1",
    "fs": "0.0.1-security",
    "js-sha256": "^0.9.0",
    "mongoose": "^5.6.8",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.1",
    "tendermint": "^4.0.8",
    "ts-node": "^8.3.0",
    "tsc-watch": "^2.4.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "winston": "^2.4.0",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.16.2",
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.1",
    "@types/config": "0.0.34",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.0",
    "@types/express-session": "1.15.6",
    "@types/jquery": "^3.3.31",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.1.31",
    "@types/mongoose": "^4.7.46",
    "@types/mongoose-paginate": "^5.0.6",
    "@types/morgan": "^1.7.36",
    "@types/node": "^10.14.14",
    "@types/winston": "^2.3.7",
    "@types/xss-filters": "0.0.27",
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.1"
  }
}
